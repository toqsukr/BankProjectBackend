generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserProfile {
  id          String   @id @map("id")
  name        String
  surname     String
  email       String   @unique
  balance     Decimal  @default(0)
  dateOfBirth DateTime
  phone       String   @unique
}
